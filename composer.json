	"license": "BSD-2-Clause",
	"authors": [
		{
			"name": "Zhaofeng Li",
			"email": "hello@zhaofeng.li"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"ext-curl": "*",
		"ext-mbstring": "*",
		"ext-iconv": "*",
