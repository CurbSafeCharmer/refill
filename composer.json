		"masterminds/html5": "2.*",
		"twig/twig": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"Reflinks": "src/"
